<% @urls.each do |url| %>
  <div class="panel panel-default col-md-4">
    <div class="panel-body ">
      <center>
        <a class="pop" href="http://localhost:3000/<%= url.short_url %>" target="_blank" data-content="<%= url.long_url %>" rel="popover" data-placement="top" data-trigger="hover">Short.ie/<%= url.short_url %></a>
      </center>
    </div>
  </div>
<% end %>